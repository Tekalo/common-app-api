{
  "name": "common-app-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "pnpm check && swc src --copy-files --out-dir build",
    "start": "node --es-module-specifier-resolution=node ./build/server.js",
    "dev": "nodemon -e ts --exec \"pnpm build && pnpm start\"",
    "debug": "nodemon -e ts --exec \"pnpm build:debug && pnpm start:debug\"",
    "build:debug": "pnpm check && swc src --source-maps true --copy-files --out-dir build",
    "start:debug": "node --es-module-specifier-resolution=node --inspect=0.0.0.0:9229 ./build/server.js | pino-pretty -c",
    "container-install": "./scripts/container-install.sh",
    "container-uninstall": "./scripts/container-uninstall.sh",
    "format": "./scripts/prettier.sh",
    "lint": "./scripts/container-lint.sh",
    "lint:fix": "./scripts/lint-fix.sh",
    "preinstall": "npx only-allow pnpm",
    "test": "./scripts/container-test.sh",
    "prepare": "husky install",
    "prisma:seed": "docker compose run --rm api pnpx prisma db seed",
    "prisma:update": "docker compose run --rm api pnpx prisma db push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schmidtfutures/common-app-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/schmidtfutures/common-app-api/issues"
  },
  "homepage": "https://github.com/schmidtfutures/common-app-api#readme",
  "dependencies": {
    "@aws-sdk/client-ses": "3.328.0",
    "@prisma/client": "4.13.0",
    "@sentry/node": "7.51.0",
    "@sentry/types": "7.51.0",
    "@tsconfig/node18": "2.0.0",
    "@types/auth0": "3.3.2",
    "@types/cookie": "^0.5.1",
    "auth0": "3.4.0",
    "connect-pg-simple": "8.0.0",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "express-oauth2-jwt-bearer": "1.4.1",
    "husky": "8.0.3",
    "jest-mock-extended": "^3.0.3",
    "jose": "4.14.4",
    "swagger-ui-express": "4.6.3",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.56",
    "@swc/jest": "0.2.26",
    "@types/connect-pg-simple": "7.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/express-session": "1.17.7",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.5",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "cookie": "0.5.0",
    "cookie-parser": "1.4.6",
    "eslint": "8.40.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-markdownlint": "0.4.1",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "pino-pretty": "10.0.0",
    "prettier": "2.8.8",
    "prisma": "4.13.0",
    "sentry-testkit": "5.0.5",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "prisma": {
    "schema": "db/prisma/schema.prisma",
    "seed": "pnpx ts-node --transpile-only ./db/resources/seed.ts"
  }
}
