              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      },
      "get": {
        "description": "Get current applicant info",
        "responses": {
          "200": {
            "description": "Success"
          },
          "404": {
            "description": "Applicant Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/applicants/me/state": {
      "put": {
        "description": "Pause an existing applicant",
        "requestBody": {
          "required": true,
