{
  "openapi": "3.0.0",
  "info": {
    "title": "Common App API",
    "description": "Documentation for Common App REST API endpoints",
    "version": "1.0.1"
  },
  "paths": {
    "/health": {
      "get": {
        "description": "Gets health status",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": false,
                  "properties": {
                    "healthy": {
                      "type": "boolean"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/applicants": {
      "post": {
        "description": "Create a new applicant",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/applicantRequestBody"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": false,
                  "properties": {
                    "id": {
                      "type": "number"
                    },
                    "email": {
                      "type": "string"
                    },
                    "auth0Id": {
                      "type": "boolean",
                      "nullable": true
                    }
                  }
                }
              }
            }
          },
          "409": {
            "description": "Bad Input",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/applicants/:id": {
      "delete": {
        "description": "Delete an existing applicant",
        "responses": {
          "200": {
            "description": "Success"
          },
          "400": {
            "description": "Bad Input",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/applicants/:id/submissions": {
      "post": {
        "description": "Create a new applicant submission",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/applicantSubmissionRequestBody"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Bad Input",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/opportunities/batch": {
      "post": {
        "description": "Create a new opportunity",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/opportunitiesRequestBody"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/opportunitiesResponseBody"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "applicantRequestBody": {
        "additionalProperties": false,
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "pronoun": {
            "type": "string"
          },
          "preferredContact": {
            "type": "string"
          },
          "acceptedTerms": {
            "type": "boolean"
          },
          "acceptedPrivacy": {
            "type": "boolean"
          }
        }
      },
      "opportunitiesResponseBody": {
        "additionalProperties": false,
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "contactName": {
            "type": "string"
          },
          "contactEmail": {
            "type": "string"
          },
          "contactPhone": {
            "type": "string"
          },
          "orgName": {
            "type": "string"
          },
          "orgSize": {
            "type": "string"
          },
          "orgType": {
            "type": "string"
          },
          "impactAreas": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "eoe": {
            "type": "boolean"
          }
        }
      },
      "opportunitiesRequestBody": {
        "additionalProperties": false,
        "type": "object",
        "properties": {
          "contact": {
            "type": "object",
            "properties": {
              "email": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "phone": {
                "type": "string"
              }
            }
          },
          "organization": {
            "type": "object",
            "properties": {
              "impactAreas": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "name": {
                "type": "string"
              },
              "size": {
                "type": "string"
              },
              "type": {
                "type": "string"
              },
              "eoe": {
                "type": "boolean"
              }
            }
          },
          "submissions": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "fullyRemote": {
                  "type": "boolean"
                },
                "location": {
                  "type": "string"
                },
                "paid": {
                  "type": "boolean"
                },
                "roleType": {
                  "type": "string"
                },
                "positionTitle": {
                  "type": "string"
                },
                "pitchEssay": {
                  "type": "string"
                },
                "source": {
                  "type": "string"
                },
                "hoursPerWeek": {
                  "type": "string"
                },
                "employmentType": {
                  "type": "string"
                },
                "salaryRange": {
                  "type": "string"
                },
                "desiredHoursPerWeek": {
                  "type": "string"
                },
                "desiredStartDate": {
                  "type": "string",
                  "format": "date"
                },
                "desiredEndDate": {
                  "type": "string",
                  "format": "date"
                },
                "jdUrl": {
                  "type": "string"
                },
                "desiredYoe": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "desiredSkills": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "desiredSkillsOther": {
                  "type": "string"
                },
                "visaSponsorship": {
                  "type": "string"
                },
                "similarStaffed": {
                  "type": "boolean"
                },
                "desiredImpactExp": {
                  "type": "string"
                }
              }
            }
          }
        }
      },
      "applicantSubmissionRequestBody": {
        "type": "object",
        "properties": {
          "originTag": {
            "type": "string"
          },
          "lastRole": {
            "type": "string"
          },
          "lastOrg": {
            "type": "string"
          },
          "yoe": {
            "type": "string"
          },
          "skills": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "otherSkills": {
            "type": "string"
          },
          "linkedInUrl": {
            "type": "string"
          },
          "githubUrl": {
            "type": "string"
          },
          "portfolioUrl": {
            "type": "string"
          },
          "portfolioPassword": {
            "type": "string"
          },
          "resumeUrl": {
            "type": "string"
          },
          "resumePassword": {
            "type": "string"
          },
          "hoursPerWeek": {
            "type": "string"
          },
          "interestEmploymentType": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "interestRoles": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "currentLocation": {
            "type": "string"
          },
          "openToRelocate": {
            "type": "string"
          },
          "openToRemote": {
            "type": "string"
          },
          "desiredSalary": {
            "type": "string"
          },
          "interestCauses": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "otherCause": {
            "type": "array"
          },
          "workAuthorization": {
            "type": "string"
          },
          "interestGovt": {
            "type": "boolean"
          },
          "interestGovtEmplTypes": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "previousImpactExperience": {
            "type": "boolean"
          },
          "essayResponse": {
            "type": "string"
          },
          "referenceAttribution": {
            "type": "string"
          }
        },
        "required": [
          "originTag",
          "lastRole",
          "lastOrg",
          "yoe",
          "skills",
          "resumeUrl",
          "interestEmploymentType",
          "interestRoles",
          "currentLocation",
          "openToRelocate",
          "openToRemote",
          "interestCauses",
          "workAuthorization",
          "interestGovt",
          "previousImpactExperience",
          "essayResponse",
          "referenceAttribution"
        ]
      }
    }
  }
}
